<!--pages/cart/childCpns/bottom-bar/bottom-bar.wxml-->
<view class='bottom-bar'>
	<view class='check-box'>
		<w-check-icon icon="icon" isChecked="{{selected}}" bindtap='CheckAll' />
		<view>全选</view>
	</view>
	<view class='counter'>
		总计: ¥{{price}}
	</view>
	<view class='payment' catchtap="settleAccounts">去结算({{counter}})</view>
</view>