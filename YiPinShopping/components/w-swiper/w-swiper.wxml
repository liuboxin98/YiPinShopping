<!--components/w-swiper/w-swiper.wxml-->
<swiper class='swiper swiper-height' indicator-dots='true' indicator-active-color='#000000' autoplay='true' circular='true' interval='3000'>
	<block wx:for="{{banners}}" wx:key="index">
		<swiper-item>
			<image class="swiper-image" src="{{item.image}}" mode='widthFix' />
		</swiper-item>
	</block>
</swiper>